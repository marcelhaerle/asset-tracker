# Environment variables for the Asset Tracker application

# Database
DATABASE_URL="file:../db/dev.db"

# Authentication
# This should be a secure random string of at least 32 characters in production
# You can generate one using: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
SESSION_SECRET="replace-with-your-own-secret-key-at-least-32-chars"

# Application settings
# Set to "production" in production environment
APP_ENV="development"